    "from sklearn.naive_bayes import GaussianNB\n",
    "import numpy as np\n",
    "x = np.array([[1,2],[1,3],[2,3],[3,1],[3,2],[3,6],[-1,-3],[3,-2],[-2,-1]])\n",
    "y = np.array([0,0,0,1,1,0,1,1,0])\n",
    "clf = GaussianNB()\n",
    "clf.fit(x,y)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "metadata": {},
   "outputs": [
    {
